{{#each this.filteredUserFields as |userField|}}
  <TableHeaderToggle
    @class="directory-table__column-header--user-field-{{userField.id}}"
    @order={{this.order}}
    @asc={{this.asc}}
    @translated="true"
    @field={{userField.description}}
    @labelKey={{userField.name}}
    @automatic={{true}}
  />
{{/each}}
